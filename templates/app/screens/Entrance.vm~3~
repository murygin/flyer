<script language="JavaScript">
sprechblase_jp = new Image();
		sprechblase_de = new Image();
		sprechblase_en = new Image();
		sprechblase_blank = new Image();


		function preloadImages(){
    	if (document.images) {
			sprechblase_jp.src="/mag/img/splash/sprechblase_jp.gif";
			sprechblase_de.src="/mag/img/splash/sprechblase_de.gif";
			sprechblase_en.src="/mag/img/splash/sprechblase_en.gif";
			sprechblase_blank.src="/mag/img/splash/sprechblase_empty.gif";
			}
		}

		function swapImages(imageName, otherName){
    	if (document.images) {
        	for (var i=0; i<swapImages.arguments.length; i+=2){
            	eval("document." + imageName + ".src = " + otherName + ".src");
        		}
			}
 		}

    function loadPage( ) {
      var nEditionId = document.forms['edsel'].edition_id.options[document.forms['edsel'].edition_id.options.selectedIndex].value;
      alert( nEditionId );
      if( nEditionId==1 )
        document.forms['edsel'].submit();
      }
    }
		</script>

