/* generelle formate */

body
	{ 
		margin: 0px; 
		background-color: white;
		font-family: Verdana, Geneva, Arial, Helvetica, Swiss, SunSans-Regular, sans-serif;
	}
	
td
	{ 
		font-family: Verdana, Geneva, Arial, Helvetica, Swiss, SunSans-Regular, sans-serif;
		font-size: 10px;
	}
	
br
	{ 
		font-family: Verdana, Geneva, Arial, Helvetica, Swiss, SunSans-Regular, sans-serif;
		font-size: 5px;
	}
	

/* formate fuer links */

a.magenta:link
	{ 
		color: #ff3399;
		background-color: transparent;
		text-decoration: none;
		cursor: pointer;
	}

a.magenta:visited
	{ 
		color: #c12673;
		background-color: transparent;
		text-decoration: none; 
		cursor: pointer;
	}

a.magenta:active
	{  
		color: black;
		background-color: transparent;
		text-decoration: none; 
		cursor: pointer;
	}

a.magenta:hover
	{ 
		color: #c12673;
		background-color: transparent;
		text-decoration: underline; 
		cursor: pointer;
	}
	
a.black:link
	{ 
		color: black;
		background-color: transparent;
		cursor: pointer;
	}

a.black:visited
	{ 
		color: black;
		background-color: transparent;
		cursor: pointer;
	}

a.black:active
	{  
		color: black;
		background-color: transparent;
		cursor: pointer;
	}

a.black:hover
	{ 
		color: #4d4d4d;
		background-color: transparent;
		cursor: pointer;
	}


/* formate fuer schriftgroessen */
	
.copy10
	{  
		font-size: 10px;
	}
	
.copy11
	{  
		font-size: 11px;
	}
	
.copy11white
	{ 
		font-family: Verdana, Geneva, Arial, Helvetica, Swiss, SunSans-Regular, sans-serif;
		color: white;
		font-size: 10px;
		font-weight: bold;
	}
	
.copy12
	{  
		font-size: 12px;
	}


/* formate fuer mauszeiger */

.cursorcrosshair
	{
		cursor:crosshair;
	}
	
.cursorpointer
	{
		cursor:pointer;
	}

.cursortext
	{
		cursor:text;
	}
	
.cursorhelp
	{
		cursor:help;
	}
	

